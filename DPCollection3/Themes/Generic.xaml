    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:CollectionBinding" >
    
    <ResourceDictionary.MergedDictionaries >
        <ResourceDictionary Source="DPCollection3;component/Themes/TargetGroup.xaml" />
        <!--<ResourceDictionary Source="pack://application:,,,/DPCollection3;component/Themes/TargetGroup.xaml" />-->
    </ResourceDictionary.MergedDictionaries>
    
    <Style TargetType="{x:Type local:TargetGroup}" BasedOn="{StaticResource {x:Type TextBox}}">
        <Setter Property="Template" Value="{StaticResource TargetGroupControlTemplate}" />
    </Style>


